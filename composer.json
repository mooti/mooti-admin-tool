{
    "name": "mooti/moot-platform-admin",
    "description": "Admin tool for mooti platform",
    "keywords": ["platform", "infrastructure"],
    "license": "Apache-2.0",
    "type": "project",
    "autoload": {
        "psr-4": { "Mooti\\Platform\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {"Mooti\\Test\\PHPUnit\\Platform\\": "tests/phpunit/src"}
    },
    "require": {
        "php": ">=5.5.9",
        "icanboogie/inflector": "^1.4",
        "symfony/console": "^3.0",        
        "symfony/process": "^3.0",
        "mooti/validator": "^0.0.6",
        "mooti/framework": "^0.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "bin": [
        "bin/mooti-admin"
    ]
}
